{
  "name": "blist",
  "version": "1.3.1",
  "description": "Blist is a clean and fast blog theme for your Hugo site.",
  "main": "index.js",
  "scripts": {
    "start": "cd exampleSite && hugo serve --themesDir ../..",
    "dev": "hugo server -s exampleSite --themesDir=../.. --disableFastRender",
    "build": "cd exampleSite && hugo --themesDir ../.. --gc"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.1",
    "tailwindcss": "^2.1.1"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "dependencies": {
    "sitemap-xml-parser": "^0.0.2",
    "sitemapper": "^3.2.0"
  }
}
